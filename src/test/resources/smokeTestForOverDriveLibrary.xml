<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="OverDrive Smoke Test Suite" verbose="8" parallel="methods" thread-count="2">
    <test name="Chrome QA env OverDrive Sign In Test">
        <parameter name="browser" value="Chrome"/>
        <parameter name="environment" value="qa"/>
        <classes>
            <class name="tests.overDriveTests.OverDriveUserAccessTest"/>
        </classes>
    </test>
    <test name="Firefox QA env Sign In Test">
        <parameter name="browser" value="Firefox"/>
        <parameter name="environment" value="qa"/>
        <classes>
            <class name="tests.overDriveTests.OverDriveUserAccessTest"/>
        </classes>
    </test>
    <test name="Chrome DEV env OverDrive Sign In Test">
        <parameter name="browser" value="Chrome"/>
        <parameter name="environment" value="dev"/>
        <classes>
            <class name="tests.overDriveTests.OverDriveUserAccessTest"/>
        </classes>
    </test>
    <test name="Firefox DEV env Sign In Test">
        <parameter name="browser" value="Firefox"/>
        <parameter name="environment" value="dev"/>
        <classes>
            <class name="tests.overDriveTests.OverDriveUserAccessTest"/>
        </classes>
    </test>
</suite>