<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Google, Wikipedia and Gismeteo Smoke Test Suite" verbose="10" parallel="tests" thread-count="1">
    <test name="ChromeTestForGoogle">
        <parameter name="browser" value="Chrome"/>
        <classes>
            <class name="tests.googleSearchResultsTests.GoogleSearchNotEmptyTest">
            </class>
        </classes>
    </test>
    <test name="FirefoxTestForGoogle">
        <parameter name="browser" value="Firefox"/>
        <classes>
            <class name="tests.googleSearchResultsTests.GoogleSearchNotEmptyTest">
            </class>
        </classes>
    </test>
    <test name="EdgeTestForGoogle">
        <parameter name="browser" value="Edge"/>
        <classes>
            <class name="tests.googleSearchResultsTests.GoogleSearchNotEmptyTest">
            </class>
        </classes>
    </test>
    <test name="ChromeTestForWikipedia">
        <parameter name="browser" value="Chrome"/>
        <classes>
            <class name="tests.googleSearchResultsTests.WikipediaPageTitleTest">
            </class>
        </classes>
    </test>
    <test name="FirefoxTestForWikipedia">
        <parameter name="browser" value="Firefox"/>
        <classes>
            <class name="tests.googleSearchResultsTests.WikipediaPageTitleTest">
            </class>
        </classes>
    </test>
    <test name="EdgeTestForWikipedia">
        <parameter name="browser" value="Edge"/>
        <classes>
            <class name="tests.googleSearchResultsTests.WikipediaPageTitleTest">
            </class>
        </classes>
    </test>
    <test name="ChromeTestForGismeteo">
        <parameter name="browser" value="Chrome"/>
        <classes>
            <class name="tests.gismeteoTests.GismeteoPageURLforUALocaleTest">
            </class>
        </classes>
    </test>
    <test name="FirefoxTestForGismeteo">
        <parameter name="browser" value="Firefox"/>
        <classes>
            <class name="tests.gismeteoTests.GismeteoPageURLforUALocaleTest">
            </class>
        </classes>
    </test>
    <test name="EdgeTestForGismeteo">
        <parameter name="browser" value="Edge"/>
        <classes>
            <class name="tests.gismeteoTests.GismeteoPageURLforUALocaleTest">
            </class>
        </classes>
    </test>
</suite>